[push]
	default = simple
[core]
	editor = $EDITIOR
[alias]
	set-upstream = !git branch --set-upstream-to=origin/`git symbolic-ref --short HEAD`
	cam = commit -am 
	cm = commit -m
	u = push
	d = pull
	m = merge
	dr = checkout --
	s = status
