[push]
	default = simple
[core]
	editor = vim
[alias]
	set-upstream = !git branch --set-upstream-to=origin/`git symbolic-ref --short HEAD`
	su = set-upstream
	cam = commit -am 
	cm = commit -m
	u = push
	d = pull
	m = merge
	dr = checkout --
	s = status
	co = checkout
	cob = checkout -b
	b = branch

